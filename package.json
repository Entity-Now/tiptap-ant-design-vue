{
  "name": "tiptap-ant-design-vue",
  "author": "Entity-now",
  "repository": {
    "type": "git",
    "url": "https://github.com/Entity-Now/tiptap-ant-design-vue"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "富文本编辑器",
    "Rich Text",
    "tiptap",
    "tiptap-ant-design-vue",
    "vue3 rich text"
  ],
  "description": "tiptap-ant-design-vue一款基于tiptap+ant designer的vue3富文本编辑器。",
  "private": false,
  "version": "1.0.13",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./lib/tiptap-ant-design-vue.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "dev": "vite",
    "demo:preview": "vite preview",
    "build:demo": "cross-env VITE_BUILD_TARGET=demo vite build",
    "build:lib": "vite build",
    "format": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\""
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@ctrl/tinycolor": "^4.1.0",
    "@floating-ui/dom": "^1.7.4",
    "@swiftcarrot/color-fns": "^3.2.0",
    "@tiptap/core": "^3.4.3",
    "@tiptap/extension-code-block": "^3.4.3",
    "@tiptap/extension-code-block-lowlight": "^3.4.3",
    "@tiptap/extension-color": "^3.4.3",
    "@tiptap/extension-font-family": "^3.4.3",
    "@tiptap/extension-highlight": "^3.4.3",
    "@tiptap/extension-image": "^3.4.3",
    "@tiptap/extension-link": "^3.4.3",
    "@tiptap/extension-list": "^3.4.3",
    "@tiptap/extension-table": "^3.4.3",
    "@tiptap/extension-text-align": "^3.4.3",
    "@tiptap/extension-text-style": "^3.4.3",
    "@tiptap/extension-underline": "^3.4.3",
    "@tiptap/extensions": "^3.4.3",
    "@tiptap/pm": "^3.4.3",
    "@tiptap/starter-kit": "^3.4.3",
    "@tiptap/vue-3": "^3.4.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.10",
    "@vueuse/core": "^13.0.0",
    "ant-design-vue": "^4.2.6",
    "highlight.js": "^11.11.1",
    "lodash-es": "^4.17.21",
    "lowlight": "^3.3.0",
    "menus": "link:@tiptap/vue-3/menus",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "state": "link:@tiptap/pm/state",
    "tiptap-extension-global-drag-handle": "^0.1.18",
    "vite-plugin-dts": "^4.5.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "typescript": "~5.7.2",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  }
